{
  "name": "bdi-agent",
  "version": "1.0.0",
  "description": "BDI Blocks World Simulation with goal-stack planning",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test:planner": "node planner-debug.js",
    "test:multi-agent": "node multi-agent-tests.js",
    "docker:build": "cd .. && docker build -t bdi-agent .",
    "docker:run": "cd .. && docker run -p 3000:3000 bdi-agent",
    "docker:up": "cd .. && docker compose up --build",
    "docker:up:dev": "cd .. && docker compose -f docker-compose.dev.yml up --build",
    "docker:down": "cd .. && docker compose down",
    "docker:logs": "cd .. && docker compose logs -f"
  },
  "keywords": [
    "bdi",
    "ai",
    "planning",
    "blocks-world",
    "goal-stack"
  ],
  "author": "CSIT321-BDI-Agent",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "js-son-agent": "^0.0.17",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1"
  }
}
